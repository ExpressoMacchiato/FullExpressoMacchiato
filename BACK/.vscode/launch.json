{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Production",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run", "prod"],
        "skipFiles": ["<node_internals>/**"],
        "cwd": "${workspaceFolder}"
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Launch Dev",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run", "dev"],
        "skipFiles": ["<node_internals>/**"],
        "cwd": "${workspaceFolder}"
    },
    ]
  }
